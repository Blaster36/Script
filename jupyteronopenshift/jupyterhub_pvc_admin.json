{
    "kind": "PersistentVolumeClaim",
    "apiVersion": "v1",
    "metadata": {
        "name": "pvc-admin",
        "labels": {
            "app": "jupyterhub"
        }
    },
    "spec": {
        "resources": {
            "requests": {
                "storage": "1Gi"
            }
        },
        "accessModes": [
            "ReadWriteOnce"
        ],
        "volumeName": "pv-admin"
    }
}
